{
	"manifest_version": 2,
	"name": "DouyuRoom",
	"description": "斗鱼房间的一些优化",
	"version": "1.0.0",
	"icons":{
		"16":"ico/icon.png",	//扩展在浏览器右上图标
		"48":"ico/icon.png"		//扩展程序管理里面的图标
	},
	"content_scripts":[{
		"js": ["js/content_scripts.js"],	//要注入的js
		"matches": ["https://www.douyu.com/*"],	//满足这里的页面方能注入js
		"run_at": "document_idle",
		"all_frames":true
	}]
}